<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel = "tests" thread-count="2">
	<listeners>
		<listener class-name="com.testingshastra.util.TestListener"></listener>
	</listeners>
	<test name="Chrome Test">
	<parameter name ="browser-name" value="Chrome"></parameter>
		<classes>
			<class name="com.testingshastra.UItests.homepagetests.HomePageTests" />
			<class name="com.testingshastra.UItests.IJmeet.POMHomePageTests" />
			<class name="com.testingshastra.config.BaseClass" />
		</classes>
			
	</test> <!-- Test -->
	<test name="Edge Test">
	<parameter name ="browser-name" value="Edge"></parameter>
		<classes>
			<class name="com.testingshastra.UItests.homepagetests.HomePageTests" />
			<class name="com.testingshastra.UItests.IJmeet.POMHomePageTests" />
			<class name="com.testingshastra.config.BaseClass" />
		</classes>
		</test>
</suite> <!-- Suite -->
